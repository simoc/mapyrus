# Display MapInfo Interchange Format file streets.mif in PostScript file.
#
newpage "eps", "tutorialdatasets4.eps", 40, 40
dataset "mif", "streets.mif", ""

worlds Mapyrus.dataset.min.x, Mapyrus.dataset.min.y, \
  Mapyrus.dataset.max.x, Mapyrus.dataset.max.y
while Mapyrus.fetch.more
do
  fetch
  clearpath
  addpath GEOMETRY
  # PENWIDTH and PENCOLOR attributes read from MIF file for each line.
  linestyle PENWIDTH
  color PENCOLOR
  stroke
done
