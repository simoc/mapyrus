print 'timer=', Mapyrus.timer

begin circle1
	key "point", "circle1"
	linestyle 0.1
	move -2, 0
	arc 1, 0, 0, -2, 0
	closepath
	stroke
end

begin triangle1
	key "point", "triangle1"
	linestyle 0.1
	move -1, -0.866
	draw 0, 0.866, 1, -0.866
	closepath
	stroke
end

begin square1
	key "point", "square1"
	linestyle 0.1
	box -1, -1, 1, 1
	stroke
end

begin square2
	key "point", "square2"
	linestyle 0.1
	box -1, -1, 1, 1
	fill
end

begin pentagon1
	key "point", "pentagon1"
	linestyle 0.1
	move -1, -1.376
	local i
	let i = 0
	while i < 4
	do
		rdraw 2, 0
		rotate 72
		let i = i + 1
	done
	closepath
	stroke
end

begin hexagon1
	key "point", "hexagon1"
	linestyle 0.1
	move -1, -2
	local i
	let i = 0
	while i < 5
	do
		rdraw 2, 0
		rotate 60
		let i = i + 1
	done
	closepath
	stroke
end

begin octagon1 
	key "point", "octagon1"
	linestyle 0.1
	move -0.848, -0.848
	local i
	let i = 0
	while i < 7
	do
		rdraw 1.656, 0
		rotate 45
		let i = i + 1
	done
	closepath
	stroke
end

