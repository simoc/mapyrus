begin pentagon1
	key "point", "pentagon1"
	linestyle 0.1
	move -1, -1.376
	local i
	let i = 0
	while i < 4
	do
		rdraw 2, 0
		rotate 72
		let i = i + 1
	done
	closepath
	stroke
end

begin hexagon1
	key "point", "hexagon1"
	linestyle 0.1
	move -1, -2
	local i
	let i = 0
	while i < 5
	do
		rdraw 2, 0
		rotate 60
		let i = i + 1
	done
	closepath
	stroke
end

begin octagon1 
	key "point", "octagon1"
	linestyle 0.1
	move -0.848, -0.848
	local i
	let i = 0
	while i < 7
	do
		rdraw 1.656, 0
		rotate 45
		let i = i + 1
	done
	closepath
	stroke
end

