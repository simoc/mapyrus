begin circle1
	key "point", "circle1"
	linestyle 0.1
	move -2, 0
	arc 1, 0, 0, -2, 0
	closepath
	stroke
end

begin triangle1
	key "point", "triangle1"
	linestyle 0.1
	move -1, -0.866
	draw 0, 0.866, 1, -0.866
	closepath
	stroke
end

