	#newpage "png", "allsymbols.png", 210, 297
	color "white"
	box 0, 0, Mapyrus.page.width, Mapyrus.page.height
	fill
	color "black"
	font "Helvetica", 3
	let x = 10, y = Mapyrus.page.height - 10
	while Mapyrus.key.count > 0
	do
		clearpath
		move x, y
		legend 10
		let y = y - 10
		if y < 10
		then
			let x = x + 50
			let y = Mapyrus.page.height - 10
		endif
	done
endif

