newpage "eps", "tutorialvar2.eps", 60, 20
color "black"

# Set binary sequence
#
let seq = "10110010011"
let seqlen = length(seq)

# Draw sequence, showing black bars for bits
# that have the value '1'.
#
let i = 1
while i <= seqlen
do
  if (substr(seq, i, 1) eq "1")
  then
    clearpath
    box i * 4, 5, i * 4 + 3, 15
    fill
  endif
  let i = i + 1
done
