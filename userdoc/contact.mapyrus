# Show contact e-mail address in an image to reduce chances of
# spammers getting hold of it.
#
newpage "png", "contact.png", 42, 4, 96
font 'Monospaced', Mapyrus.page.resolution.mm * 12
color '#A03060'
justify 'left'
clearpath
move 1, 1
let user = 'mapyrus'
let domain = 'chenery.id.au'
label user . '@' . domain
