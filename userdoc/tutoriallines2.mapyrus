newpage "eps", "tutoriallines2.eps", 60, 30, 96

# Demonstrate combination of solid and dashed linestyles.
#
clearpath
move 5, 5
draw 45, 5
arc -1, 45, 15, 45, 25
draw 15, 25
arc -1, 15, 20, 15, 15
draw 35, 15
linestyle 2.5, 'butt', 'round'
color 'black'
stroke
linestyle 1.8, 'butt', 'round', 1, 2, 2
color 'yellow'
stroke
